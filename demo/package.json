{
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "alita build",
    "dev": "alita dev",
    "format": "prettier --write .",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .ts,.tsx",
    "plugin": "alita plugin list",
    "start": "alita dev",
    "test": "jest"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.css": "stylelint --syntax css",
    "**/*.{ts,tsx}": "npm run lint-staged:js",
    "**/*.{ts,tsx,json,jsx,less}": [
      "git add",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@alita/flow": "^3.1.1",
    "@lingxiteam/assets": "3.3.1-alpha.13",
    "@monaco-editor/react": "^4.6.0",
    "alita": "^3.2.27",
    "antd": "^5.11.1",
    "antd-mobile": "^5.15.1",
    "archiver": "^6.0.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "ebe": "0.1.2",
    "ebe-utils": "file://../ebe-utils",
    "es6-proxy-polyfill": "^2.1.1",
    "file-saver": "^2.0.5",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@umijs/utils": "4.2.3",
    "prettier": "^2",
    "ts-node": "^10",
    "typescript": "^4"
  },
  "engines": {
    "node": ">=14.0.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "alita verify-commit"
  }
}

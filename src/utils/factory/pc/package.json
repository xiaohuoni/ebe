    "@lingxiteam/icons": "^0.0.3",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "father": "^4.1.9"
  },
  "peerDependencies": {
    "@lingxiteam/assets": "^3.3.1-alpha.13",
    "@lingxiteam/icons": "^0.0.3",
    "ahooks": "^3.0.0",
    "antd": ">=4.18.8",
    "lodash": "^4.0.0",
    "moment": "^2.29.1",
    "react": ">=16.8.6",
    "react-loadable": "^5.5.0"
  },

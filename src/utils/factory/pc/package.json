    "classnames": "^2.2.6",
    "echarts": "^5.1.2",
    "lcdp-message": "0.1.9",
    "qr-code-with-logo": "1.1.0",
    "rc-tween-one": "3.0.6",
    "react-player": "2.11.0",
    "react-quill": "^2.0.0",
    "react-resizable": "^3.0.4",
    "react-soundplayer": "^1.0.5",
    "size-sensor": " ^1.0.1"
  },
